<RelativeLayout
	android:layout_height = "match_parent"
	android:layout_width = "match_parent"
	android:paddingBottom = "@dimen/activity_vertical_margin"
	android:paddingLeft = "@dimen/activity_horizontal_margin"
	android:paddingRight = "@dimen/activity_horizontal_margin"
	android:paddingTop = "@dimen/activity_vertical_margin"
	tools:context = ".MainActivity"
	xmlns:android = "http://schemas.android.com/apk/res/android"
	xmlns:tools = "http://schemas.android.com/tools"
	>

	<TextView
		android:layout_height = "wrap_content"
		android:layout_width = "wrap_content"
		android:text = "injSingletonMyClass01"
		android:id = "@+id/tv_injSingleton"
		android:layout_below = "@+id/tv_injNon" android:layout_alignParentLeft = "true" android:layout_alignParentStart = "true"
		/>

	<TextView
		android:layout_width = "wrap_content"
		android:layout_height = "wrap_content"
		android:text = "injNonSingleton"
		android:id = "@+id/tv_injNon"
		android:layout_alignParentTop = "true" android:layout_alignParentLeft = "true" android:layout_alignParentStart = "true"
		/>

	<TextView
		android:layout_width = "wrap_content"
		android:layout_height = "wrap_content"
		android:text = "injConstructor not provided in module"
		android:id = "@+id/tv_injConst"
		android:layout_below = "@+id/tv_injSingletonAnother" android:layout_alignParentLeft = "true" android:layout_alignParentStart = "true"
		/>

	<Button
		android:layout_width = "wrap_content"
		android:layout_height = "wrap_content"
		android:text = "get from obj graph"
		android:id = "@+id/btn_getObjG"
		android:layout_below = "@+id/tv_injConst" android:layout_alignParentLeft = "true" android:layout_alignParentStart = "true"
		/>

	<TextView
		android:layout_width = "wrap_content"
		android:layout_height = "wrap_content"
		android:text = "injSingletonAnotherMyClass01"
		android:id = "@+id/tv_injSingletonAnother"
		android:layout_below = "@+id/tv_injSingleton" android:layout_alignParentLeft = "true" android:layout_alignParentStart = "true"
		/>

</RelativeLayout>
